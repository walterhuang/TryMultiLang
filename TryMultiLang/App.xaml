<Application
    x:Class="TryMultiLang.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:TryMultiLang"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources\StringResources.es-MX.xaml" />
                <ResourceDictionary Source="Resources\StringResources.vi-VN.xaml" />
                <ResourceDictionary Source="Resources\StringResources.zh-CN.xaml" />
                <ResourceDictionary Source="Resources\StringResources.zh-TW.xaml" />
                <ResourceDictionary Source="Resources\StringResources.en-US.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>